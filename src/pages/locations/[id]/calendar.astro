---
import AppLayout from "../../../layouts/AppLayout.astro";
import { CalendarView } from "@/components/locations/calendar/CalendarView";

const { id } = Astro.params;

if (!id) {
  return Astro.redirect("/");
}
---

<AppLayout title="Kalendarz">
  <div class="space-y-6">
    <CalendarView client:load locationId={id} />
  </div>
</AppLayout>
